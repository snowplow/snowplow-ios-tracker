sion:onsuccess:)"],"traits":[{"interfaceLanguage":"swift"}]}],"identifier":{"url":"doc:\/\/SnowplowTracker\/documentation\/SnowplowTracker\/Snowplow\/setup(remoteConfiguration:defaultConfiguration:defaultConfigurationVersion:onSuccess:)","interfaceLanguage":"swift"},"abstract":[{"type":"text","text":"Setup a single or a set of tracker instances which will be used inside the app to track events."}],"kind":"symbol","primaryContentSections":[{"declarations":[{"tokens":[{"text":"@objc","kind":"attribute"},{"text":" ","kind":"text"},{"kind":"keyword","text":"class"},{"text":" ","kind":"text"},{"kind":"keyword","text":"func"},{"text":" ","kind":"text"},{"kind":"identifier","text":"setup"},{"text":"(","kind":"text"},{"kind":"externalParam","text":"remoteConfiguration"},{"text":": ","kind":"text"},{"preciseIdentifier":"c:@M@SnowplowTracker@objc(cs)SPRemoteConfiguration","text":"RemoteConfiguration","kind":"typeIdentifier","identifier":"doc:\/\/SnowplowTracker\/documentation\/SnowplowTracker\/RemoteConfiguration"},{"text":", ","kind":"text"},{"text":"defaultConfiguration","kind":"externalParam"},{"text":" ","kind":"text"},{"text":"defaultBundles","kind":"internalParam"},{"kind":"text","text":": ["},{"text":"ConfigurationBundle","kind":"typeIdentifier","preciseIdentifier":"c:@M@SnowplowTracker@objc(cs)SPConfigurationBundle","identifier":"doc:\/\/SnowplowTracker\/documentation\/SnowplowTracker\/ConfigurationBundle"},{"text":"]?, ","kind":"text"},{"kind":"externalParam","text":"defaultConfigurationVersion"},{"kind":"text","text":" "},{"kind":"internalParam","text":"defaultBundleVersion"},{"text":": ","kind":"text"},{"kind":"typeIdentifier","preciseIdentifier":"s:Si","text":"Int"},{"kind":"text","text":" = NSInteger.min, "},{"kind":"externalParam","text":"onSuccess"},{"kind":"text","text":": "},{"text":"@escaping","kind":"keyword"},{"text":" ([","kind":"text"},{"kind":"typeIdentifier","preciseIdentifier":"s:SS","text":"String"},{"text":"]?, ","kind":"text"},{"kind":"typeIdentifier","identifier":"doc:\/\/SnowplowTracker\/documentation\/SnowplowTracker\/ConfigurationState","text":"ConfigurationState","preciseIdentifier":"c:@M@SnowplowTracker@E@SPConfigurationState"},{"text":") -> ","kind":"text"},{"text":"Void","kind":"typeIdentifier","preciseIdentifier":"s:s4Voida"},{"kind":"text","text":")"}],"languages":["swift"],"platforms":["macOS"]}],"kind":"declarations"},{"parameters":[{"content":[{"type":"paragraph","inlineContent":[{"text":"The remote configuration used to indicate where to download the configuration from.","type":"text"}]}],"name":"remoteConfiguration"},{"content":[{"type":"paragraph","inlineContent":[{"text":"The default configuration passed by default in case there isn’t a cached version and it’s able to download a new one.","type":"text"}]}],"name":"defaultBundles"},{"name":"defaultBundleVersion","content":[{"type":"paragraph","inlineContent":[{"text":"Version of the default configuration that will be used to compare with the fetched remote config to decide whether to replace it.","type":"text"}]}]},{"name":"onSuccess","content":[{"type":"paragraph","inlineContent":[{"text":"The callback called when a configuration (cached or downloaded) is set.","type":"text"},{"type":"text","text":" "},{"text":"It passes two arguments: list of the namespaces associated to the created trackers","type":"text"},{"text":" ","type":"text"},{"text":"and the state of the configuration – whether it was retrieved from cache or fetched over the network.","type":"text"}]}]}],"kind":"parameters"},{"kind":"content","content":[{"level":2,"text":"Discussion","anchor":"discussion","type":"heading"},{"inlineContent":[{"text":"The app can run multiple tracker instances which will be identified by string ","type":"text"},{"code":"namespaces","type":"codeVoice"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"The trackers configuration is automatically download from the endpoint indicated in the "},{"type":"codeVoice","code":"RemoteConfiguration"},{"type":"text","text":" "},{"type":"text","text":"passed as argument. For more details see "},{"code":"RemoteConfiguration","type":"codeVoice"},{"type":"text","text":"."}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"text":"The method is asynchronous and you can receive the list of the created trackers in the callbacks once the trackers are created.","type":"text"},{"type":"text","text":" "},{"type":"text","text":"The callback can be called multiple times in case a cached configuration is ready and later a fetched configuration is available."},{"type":"text","text":" "},{"type":"text","text":"You can also pass as argument a default configuration in case there isn’t a cached configuration and it’s not able to download"},{"type":"text","text":" "},{"type":"text","text":"a new one. The downloaded configuration updates the cached one only if the configuration version is greater than the cached one."},{"type":"text","text":" "},{"type":"text","text":"Otherwise the cached one is kept and the callback is not called."}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"IMPORTANT: The EventStore will persist all the events that have been tracked but not yet sent."},{"type":"text","text":" "},{"text":"Those events are attached to the namespace.","type":"text"},{"text":" ","type":"text"},{"text":"If the tracker is removed or the app relaunched with a different namespace, those events can’t","type":"text"},{"text":" ","type":"text"},{"text":"be sent to the collector and they remain in a zombie state inside the EventStore.","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"To remove all the zombie events you can call an internal method "},{"type":"codeVoice","code":"removeUnsentEventsExceptForNamespaces"},{"type":"text","text":" on "},{"type":"codeVoice","code":"SPSQLEventStore"},{"text":" ","type":"text"},{"type":"text","text":"which will delete all the EventStores instanced with namespaces not listed in the passed list."}]}]}],"references":{"doc://SnowplowTracker/documentation/SnowplowTracker":{"title":"SnowplowTracker","identifier":"doc:\/\/SnowplowTracker\/documentation\/SnowplowTracker","role":"collection","kind":"symbol","url":"\/documentation\/snowplowtracker","type":"topic","abstract":[]},"doc://SnowplowTracker/documentation/SnowplowTracker/Snowplow":{"title":"Snowplow","kind":"symbol","url":"\/documentation\/snowplowtracker\/snowplow","abstract":[{"text":"Entry point to instance a new Snowplow tracker.","type":"text"}],"identifier":"doc:\/\/SnowplowTracker\/documentation\/SnowplowTracker\/Snowplow","fragments":[{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"kind":"identifier","text":"Snowplow"}],"role":"symbol","navigatorTitle":[{"kind":"identifier","text":"Snowplow"}],"type":"topic"},"doc://SnowplowTracker/documentation/SnowplowTracker/RemoteConfiguration":{"kind":"symbol","url":"\/documentation\/snowplowtracker\/remoteconfiguration","type":"topic","abstract":[{"type":"text","text":"Represents the configuration for fetching configurations from a remote source."},{"text":" ","type":"text"},{"text":"For details on the correct format of a remote configuration see the official documentation.","type":"text"}],"fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"RemoteConfiguration"}],"identifier":"doc:\/\/SnowplowTracker\/documentation\/SnowplowTracker\/RemoteConfiguration","role":"symbol","navigatorTitle":[{"text":"RemoteConfiguration","kind":"identifier"}],"title":"RemoteConfiguration"},"doc://SnowplowTracker/documentation/SnowplowTracker/ConfigurationBundle":{"identifier":"doc:\/\/SnowplowTracker\/documentation\/SnowplowTracker\/ConfigurationBundle","title":"ConfigurationBundle","type":"topic","abstract":[{"text":"This class represents the default configuration applied in place of the remote configuration.","type":"text"}],"role":"symbol","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{